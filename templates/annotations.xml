<tei:TEI 
    xmlns:tei="http://www.tei-c.org/ns/1.0" 
    xmlns:j="http://jewishliturgy.org/ns/jlptei/1.0" 
    xml:lang="??">
    <!--      ^^ Fill in the primary language above. 
                 Use "en" for English, "he" for Hebrew, "arc" for Aramaic  
    -->
    <tei:teiHeader>
        <tei:fileDesc>
            <tei:titleStmt>
                <tei:title type="main" xml:lang="??"><!--?? FILL in the main title here --></tei:title>
                <!-- An "alt" title (such as a translated title) and a subtitle are optional:
                <tei:title type="alt" xml:lang="??">??</tei:title>
                <tei:title type="sub" xml:lang="??">??</tei:title>
                -->
                <!-- ?? 
                    If you need to assign credit for work (such as transcription, editing, or proofreading
                    done by someone other than yourself, it is assigned using responsibility statements,
                    as in the example below:
                <tei:respStmt>
                    <tei:resp key="trc">Transcribed by</tei:resp>
                    <tei:name ref="/user/somebody">Somebody's Name</tei:name>
                </tei:respStmt>
                -->
            </tei:titleStmt>
            <tei:publicationStmt>
                <tei:distributor>
                    <tei:ref target="http://opensiddur.org">Open Siddur Project</tei:ref>
                </tei:distributor>
                <tei:availability>
                    <tei:licence target="http://www.creativecommons.org/publicdomain/zero/1.0"/><!-- <- CC0
                    <tei:licence target="http://www.creativecommons.org/licenses/by/4.0"/> <- CC-BY 4.0
                    <tei:licence target="http://www.creativecommons.org/licenses/by-sa/4.0"/> <- CC-BY-SA 4.0
                    -->
                </tei:availability>
                <tei:date></tei:date>
            </tei:publicationStmt>
            <tei:sourceDesc>
                <tei:link type="bibl" target="#ann /data/sources/Born%20Digital"/>
                <!--                                              ^^^ fill in the link to the bibliography entry -->
            </tei:sourceDesc>
        </tei:fileDesc>
        <tei:revisionDesc>
        </tei:revisionDesc>
    </tei:teiHeader>
    <j:annotations xml:id="ann">
        <tei:idno><!-- ??name of the annotation set: this is how the annotations are referenced from other documents --></tei:idno>
        <!-- ??note annotation: possible types are instruction, comment -->
        <tei:note xml:id="??" type="??"><!-- ?? --></tei:note>
        <!-- ??fs annotation: give text some property, as in this example: -->
        <tei:fs xml:id="??" type="historical">
            <tei:f name="author"></tei:f>
            <tei:f name="composition-date"></tei:f>
        </tei:fs>
        <!-- ??interp annotation: -->
        <tei:interp xml:id="??" type="historical-period">gaonic</tei:interp>
    </j:annotations>
</tei:TEI>
